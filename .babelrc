{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": "current"
      }
    }],
    ["@babel/preset-react"]
  ],
  "env": {
    "test": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "node": "current"
          }
        }]
      ]
    }
  },
  "plugins": [
    "@babel/plugin-transform-runtime",
    [
      "import",
      {
          "libraryName": "antd",
          "libraryDirectory": "dist",
          "style": true
      }
  ]]
}